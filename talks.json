---
permalink: /talks.json
layout: none
---
[
{% assign talks = site.talks | where_exp: "t", "t.latitude and t.longitude" | sort: "date" | reverse %}
{% for t in talks %}
  {% if forloop.index0 > 0 %},{% endif %}
  {
    "title": {{ t.title | jsonify }},
    "venue": {{ t.venue | default: "" | jsonify }},
    "where": {{ t.location | default: "" | jsonify }},
    "url": {{ t.url | relative_url | jsonify }},
    "lat": {{ t.latitude | jsonify }},
    "lng": {{ t.longitude | jsonify }},
    "date": {{ t.date | date: "%Y-%m-%d" | jsonify }}
  }
{% endfor %}
]
